
{% extends "AppBundle:Layout:templategeneral.html.twig" %}

{% block body %}




<section class="toutmillieu">

  <div class="millieu">
  <article class="arti">
     {% for category in listCategory %}
                  <div class="col-sm-4 col-lg-6 col-md-6">
      <div class="thumbnail">
<a href="{{ path('productcatalog', {'id': category.id, 'idteam': idteam} )}}"><img src="{{asset('uploads/images/category/'~category.image)}}" height="180px"  width="140px" ></a>
          
        <div class="caption">
                          
            <h4>{{category.libelle}}  </h4>
         
            <p>{{category.commentaire}}</p>
        </div>
        

    </div>
 </div>


        {% endfor %}
    
  </article>




</section>

{% endblock %}
